function ext2mime( $file_ext = '' ) {
    global $MWJma;
  //  if( !is_string( $fileName ) ) { $fileName = ''; }
   // $fileName = preg_replace( "/.*([^.\w-][\w-]*|\.([\w-]*))$/", "$2", strtolow
    if( !$file_ext || !$MWJma[$file_ext] ) { return 'application/octet-stream'; } else { return $MWJma[$file_ext]; }
}
                    

